{
  "name": "kode",
  "version": "0.0.0",
  "scripts": {
    "config": "esbuild --bundle --tree-shaking=true --mangle-props=_$ --reserve-props=^__.*__$ --splitting --chunk-names=chunks/[name] --format=esm",
    "dev": "npm run config -- src/main.ts --sourcemap --servedir=public",
    "build": "npm run config -- src/kode.ts --minify --legal-comments=linked --drop:console --outdir=public/dist --analyze"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.0",
    "esbuild": "^0.14.23",
    "typescript": "^4.5.5"
  }
}
